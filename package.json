        "@vue/test-utils": "^1.2.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "26.6.3",
        "jest": "26.6.3",
        "node-sass": "^4.12.0",
        "sass-loader": "^8.0.2",
        "vue-jest": "^3.0.7",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": [
